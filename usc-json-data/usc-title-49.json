        {
          "id": "/us/usc/t49/stVIII/ch605",
          "number": null,
          "name": "INTERSTATE COMMERCE REGULATION",
          "sections": [
            {
              "id": "/us/usc/t49/s60501",
              "number": null,
              "heading": "Secretary of Energy",
              "text": "Except as provided in , the Secretary of Energy has the duties and powers related to the transportation of oil by pipeline that were vested on , in the Interstate Commerce Commission or the chairman or a member of the Commission.",
              "subsections": []
            },
            {
              "id": "/us/usc/t49/s60502",
              "number": null,
              "heading": "Federal Energy Regulatory Commission",
              "text": "The Federal Energy Regulatory Commission has the duties and powers related to the establishment of a rate or charge for the transportation of oil by pipeline or the valuation of that pipeline that were vested on , in the Interstate Commerce Commission or an officer or component of the Interstate Commerce Commission.",
              "subsections": []
            },
            {
              "id": "/us/usc/t49/s60503",
              "number": null,
              "heading": "Effect of enactment",
              "text": "The enactment of the Act of  (, ), the Act of  (, ), and the Act enacting this section does not repeal, and has no substantive effect on, any right, obligation, liability, or remedy of an oil pipeline, including a right, obligation, liability, or remedy arising under the Interstate Commerce Act or the Act of  (known as the Pomerene Bills of Lading Act), before any department, agency, or instrumentality of the United States Government, an officer or employee of the Government, or a court of competent jurisdiction.",
              "subsections": []
            }
          ]
        }
